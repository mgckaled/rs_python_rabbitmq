[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[scripts]
lint = "pylint --rcfile=.pylintrc src/"
lint-report = "pylint --rcfile=.pylintrc --report=y src/"
test = "pytest"
test-list = "pytest -s -v"

[packages]
pika = "*"
requests = "*"

[dev-packages]
pytest = "*"
pylint = "*"

[requires]
python_version = "3.11"
python_full_version = "3.11.9"
